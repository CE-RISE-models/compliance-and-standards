@prefix comp: <https://ce-rise-models.codeberg.page/compliance-and-standards/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

comp:ComplianceAndStandards a sh:NodeShape ;
    sh:closed true ;
    sh:description "Root container for all compliance and standards related information, encompassing commitments, requirements, evidence, and regulatory data." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class comp:ProductCommitments ;
            sh:description "All product-related commitments (regulatory, voluntary, contractual)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path comp:product_commitments ],
        [ sh:class comp:ProductEvidence ;
            sh:description "Documentary evidence supporting compliance claims" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path comp:product_evidence ],
        [ sh:class comp:ApplicableRegulationStandards ;
            sh:description "Applicable regulations, standards, and specifications" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path comp:applicable_regulation_standards ],
        [ sh:class comp:RegulatoryInformation ;
            sh:description "Structured regulatory information including safety, instructions, and identifiers" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path comp:regulatory_information ],
        [ sh:class comp:ProcessEvidence ;
            sh:description "Management system and process-related evidence" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path comp:process_evidence ] ;
    sh:targetClass comp:ComplianceAndStandards .

comp:Amendment a sh:NodeShape ;
    sh:closed true ;
    sh:description "Record of an amendment to a requirement" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Description of changes" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:amendment_description ],
        [ sh:datatype xsd:string ;
            sh:description "Type of amendment (correction, supplement, revision)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:amendment_type ],
        [ sh:datatype xsd:date ;
            sh:description "Date of amendment" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:amendment_date ],
        [ sh:datatype xsd:string ;
            sh:description "Amendment identifier" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:amendment_id ] ;
    sh:targetClass comp:Amendment .

comp:ApplicabilityStatus a sh:NodeShape ;
    sh:closed true ;
    sh:description "Jurisdiction-specific applicability status for a requirement, allowing tracking of how requirements apply differently across markets." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:date ;
            sh:description "Date when this status ends or changes" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:end_date ],
        [ sh:datatype xsd:string ;
            sh:description "Level of enforcement in this jurisdiction" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path comp:enforcement_level ],
        [ sh:datatype xsd:string ;
            sh:description "Transition period details if applicable" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:transition_period ],
        [ sh:datatype xsd:date ;
            sh:description "Date when this status becomes effective" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:effective_date ],
        [ sh:datatype xsd:string ;
            sh:description "Any derogations or exemptions" ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path comp:derogations ],
        [ sh:description "Status in this jurisdiction" ;
            sh:in ( "MANDATORY" "PRESUMPTION_OF_CONFORMITY" "RECOMMENDED" "VOLUNTARY" "SUPERSEDED" "WITHDRAWN" "DRAFT" "UNDER_REVIEW" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path comp:status ],
        [ sh:datatype xsd:string ;
            sh:description "Jurisdiction where this status applies" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:jurisdiction ] ;
    sh:targetClass comp:ApplicabilityStatus .

comp:ApplicableRegulationStandards a sh:NodeShape ;
    sh:closed true ;
    sh:description "Container for all applicable regulations, standards, and specifications with jurisdiction-aware requirements tracking and version management." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class comp:RequirementEntry ;
            sh:description "List of applicable requirements (regulations, standards, specifications)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path comp:requirement_entries ] ;
    sh:targetClass comp:ApplicableRegulationStandards .

comp:AuditRecord a sh:NodeShape ;
    sh:closed true ;
    sh:description "Record of an audit" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Corrective actions taken" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:corrective_actions ],
        [ sh:datatype xsd:string ;
            sh:description "Overall audit result" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:audit_result ],
        [ sh:description "Type of audit" ;
            sh:in ( "INITIAL" "SURVEILLANCE" "RECERTIFICATION" "SPECIAL" "INTERNAL" "CUSTOMER" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path comp:audit_type ],
        [ sh:datatype xsd:string ;
            sh:description "Audit findings" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:findings ],
        [ sh:datatype xsd:date ;
            sh:description "Date of audit" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:audit_date ],
        [ sh:datatype xsd:string ;
            sh:description "Auditor or audit organization" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path comp:auditor ] ;
    sh:targetClass comp:AuditRecord .

comp:CertificationDetails a sh:NodeShape ;
    sh:closed true ;
    sh:description "Details about management system certification" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class comp:ValidityPeriod ;
            sh:description "Validity period of certification" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path comp:validity_period ],
        [ sh:datatype xsd:string ;
            sh:description "Certificate number" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:certificate_number ],
        [ sh:datatype xsd:string ;
            sh:description "Accreditation details of certification body" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:accreditation_details ],
        [ sh:datatype xsd:string ;
            sh:description "Name of certification body" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:certification_body ],
        [ sh:description "Current certification status" ;
            sh:in ( "CERTIFIED" "PENDING" "EXPIRED" "SUSPENDED" "WITHDRAWN" "IN_PROGRESS" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path comp:certification_status ] ;
    sh:targetClass comp:CertificationDetails .

comp:Commitment a sh:NodeShape ;
    sh:closed true ;
    sh:description "A single commitment representing any type of product-related obligation, promise, or compliance requirement with flexible categorization and jurisdiction-specific status tracking." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Version tracking for this commitment" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path comp:version_info ],
        [ sh:class comp:ComplianceStatus ;
            sh:description "Status of this commitment in different jurisdictions" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path comp:compliance_statuses ],
        [ sh:class comp:StakeholderScope ;
            sh:description "Scope of applicability for this commitment" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path comp:stakeholder_scope ],
        [ sh:class comp:CommitmentCategory ;
            sh:description "Categories this commitment belongs to (can be multiple)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path comp:commitment_categories ],
        [ sh:datatype xsd:string ;
            sh:description "Detailed description or context of the commitment" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:commitment_description ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Last update timestamp for this commitment" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path comp:last_updated ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier for this commitment" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:commitment_identifier ],
        [ sh:class comp:EvidenceReferences ;
            sh:description "References to supporting evidence for this commitment" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path comp:evidence_references ],
        [ sh:class comp:ValidityPeriod ;
            sh:description "Period during which this commitment is valid" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path comp:validity_period ],
        [ sh:datatype xsd:string ;
            sh:description "The actual commitment statement or text" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:commitment_statement_value ] ;
    sh:targetClass comp:Commitment .

comp:CommitmentCategory a sh:NodeShape ;
    sh:closed true ;
    sh:description "Category or type of commitment with external standard references" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Type of commitment category" ;
            sh:in ( "REGULATORY" "VOLUNTARY" "CONTRACTUAL" "INDUSTRY_STANDARD" "CUSTOMER_REQUIREMENT" "ENVIRONMENTAL" "SOCIAL" "QUALITY" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path comp:category_type ],
        [ sh:datatype xsd:string ;
            sh:description "External ontology or standard reference for this category" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:category_reference ],
        [ sh:datatype xsd:integer ;
            sh:description "Priority or importance level of this category" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:priority_level ] ;
    sh:targetClass comp:CommitmentCategory .

comp:ComplianceStatement a sh:NodeShape ;
    sh:closed true ;
    sh:description "A regulatory compliance statement" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Jurisdictions where valid" ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:valid_for_jurisdictions ],
        [ sh:datatype xsd:string ;
            sh:description "References to supporting tests" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:test_references ],
        [ sh:description "Type of statement" ;
            sh:in ( "EMC" "SAFETY" "ENVIRONMENTAL" "RADIO" "CHEMICAL" "ENERGY" "PRIVACY" "ACCESSIBILITY" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path comp:statement_type ],
        [ sh:datatype xsd:string ;
            sh:description "Standards this statement relates to" ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:applicable_standards ],
        [ sh:datatype xsd:string ;
            sh:description "Content of the statement" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:statement_content ] ;
    sh:targetClass comp:ComplianceStatement .

comp:ComplianceStatements a sh:NodeShape ;
    sh:closed true ;
    sh:description "Container for compliance statements" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class comp:ComplianceStatement ;
            sh:description "Individual compliance statements" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path comp:statements ] ;
    sh:targetClass comp:ComplianceStatements .

comp:ComplianceStatus a sh:NodeShape ;
    sh:closed true ;
    sh:description "Jurisdiction-specific compliance status for a commitment, allowing the same commitment to have different statuses in different markets." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:date ;
            sh:description "Date when this status became effective" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:effective_date ],
        [ sh:datatype xsd:string ;
            sh:description "Jurisdiction or market where this status applies" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:jurisdiction ],
        [ sh:description "Current status in this jurisdiction" ;
            sh:in ( "MANDATORY" "VOLUNTARY" "RECOMMENDED" "OPTIONAL" "NOT_APPLICABLE" "PENDING" "SUPERSEDED" "TRANSITIONAL" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path comp:status_value ],
        [ sh:datatype xsd:string ;
            sh:description "Additional notes or context about this status" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:status_notes ],
        [ sh:datatype xsd:string ;
            sh:description "Authority that determined this status" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path comp:issuing_authority ],
        [ sh:datatype xsd:date ;
            sh:description "Date when this status expires or changes" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:expiry_date ] ;
    sh:targetClass comp:ComplianceStatus .

comp:DigitalSignature a sh:NodeShape ;
    sh:closed true ;
    sh:description "Digital signature information for evidence authenticity" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:dateTime ;
            sh:description "Date of digital signature" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:signature_date ],
        [ sh:datatype xsd:string ;
            sh:description "Type of digital signature" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:signature_type ],
        [ sh:datatype xsd:string ;
            sh:description "Digital signature value or hash" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:signature_value ],
        [ sh:datatype xsd:string ;
            sh:description "Reference to digital certificate" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:certificate_reference ],
        [ sh:datatype xsd:string ;
            sh:description "Person or entity who signed" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:signatory ] ;
    sh:targetClass comp:DigitalSignature .

comp:DocumentAccess a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about document access and storage" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Checksum for integrity verification" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:checksum ],
        [ sh:datatype xsd:string ;
            sh:description "Format of the document" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:document_format ],
        [ sh:datatype xsd:string ;
            sh:description "URL where document can be accessed" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:document_url ],
        [ sh:datatype xsd:string ;
            sh:description "Any access restrictions" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:access_restrictions ],
        [ sh:datatype xsd:string ;
            sh:description "Where document is stored" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path comp:storage_location ],
        [ sh:datatype xsd:string ;
            sh:description "How long document must be retained" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:retention_period ] ;
    sh:targetClass comp:DocumentAccess .

comp:EvidenceDocument a sh:NodeShape ;
    sh:closed true ;
    sh:description "A single piece of documentary evidence supporting compliance claims, with version tracking, digital signatures, and scope definition." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class comp:DigitalSignature ;
            sh:description "Digital signature information" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path comp:digital_signature ],
        [ sh:class comp:ScopeOfEvidence ;
            sh:description "Scope covered by this evidence" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path comp:scope_of_evidence ],
        [ sh:datatype xsd:string ;
            sh:description "Commitments that this evidence supports" ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path comp:commitment_support ],
        [ sh:description "Type of evidence document" ;
            sh:in ( "DECLARATION_OF_CONFORMITY" "CERTIFICATE" "TEST_REPORT" "INSPECTION_REPORT" "MARKING" "ATTESTATION" "TYPE_EXAMINATION" "APPROVAL" "TECHNICAL_FILE" "VERIFICATION" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path comp:evidence_type ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier for this document" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:document_identifier ],
        [ sh:datatype xsd:date ;
            sh:description "Date when document was issued" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:issue_date ],
        [ sh:datatype xsd:string ;
            sh:description "Language(s) of the document" ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path comp:document_language ],
        [ sh:class comp:ValidityPeriod ;
            sh:description "Validity period of this evidence" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path comp:validity_period ],
        [ sh:datatype xsd:string ;
            sh:description "Conformity assessment procedure used" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path comp:conformity_assessment_procedure ],
        [ sh:class comp:IssuingAuthority ;
            sh:description "Details about the issuing authority" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path comp:issuing_authority_details ],
        [ sh:datatype xsd:string ;
            sh:description "Title of the document" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:document_title ],
        [ sh:class comp:DocumentAccess ;
            sh:description "Document access and storage information" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path comp:document_access ] ;
    sh:targetClass comp:EvidenceDocument .

comp:EvidenceReferences a sh:NodeShape ;
    sh:closed true ;
    sh:description "References to evidence supporting a commitment" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "References to audit records" ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:audit_records ],
        [ sh:datatype xsd:string ;
            sh:description "References to test reports" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:test_reports ],
        [ sh:datatype xsd:string ;
            sh:description "URIs or identifiers of supporting documents" ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:supporting_documents ],
        [ sh:datatype xsd:string ;
            sh:description "Links to relevant certifications" ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:certification_links ] ;
    sh:targetClass comp:EvidenceReferences .

comp:ImprovementTarget a sh:NodeShape ;
    sh:closed true ;
    sh:description "An improvement target" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Description of target" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:target_description ],
        [ sh:datatype xsd:date ;
            sh:description "Target achievement date" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:target_date ],
        [ sh:datatype xsd:float ;
            sh:description "Target value" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:target_value ] ;
    sh:targetClass comp:ImprovementTarget .

comp:IssuingAuthority a sh:NodeShape ;
    sh:closed true ;
    sh:description "Details about the authority issuing evidence" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Country of the authority" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:authority_country ],
        [ sh:datatype xsd:string ;
            sh:description "Notified body number if applicable" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path comp:notified_body_number ],
        [ sh:datatype xsd:string ;
            sh:description "Accreditation information" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:accreditation_details ],
        [ sh:description "Type of authority" ;
            sh:in ( "NOTIFIED_BODY" "CERTIFICATION_BODY" "TEST_LABORATORY" "INSPECTION_BODY" "MANUFACTURER" "AUTHORIZED_REPRESENTATIVE" "COMPETENT_AUTHORITY" "ACCREDITATION_BODY" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path comp:authority_type ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier for the authority" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:authority_identifier ],
        [ sh:datatype xsd:string ;
            sh:description "Name of the issuing authority" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:authority_name ] ;
    sh:targetClass comp:IssuingAuthority .

comp:KeyPerformanceIndicator a sh:NodeShape ;
    sh:closed true ;
    sh:description "A key performance indicator" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Unit of measurement" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:kpi_unit ],
        [ sh:datatype xsd:string ;
            sh:description "Name of the KPI" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:kpi_name ],
        [ sh:datatype xsd:string ;
            sh:description "Period of measurement" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:measurement_period ],
        [ sh:datatype xsd:float ;
            sh:description "Current value" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path comp:kpi_value ] ;
    sh:targetClass comp:KeyPerformanceIndicator .

comp:ManagementSystem a sh:NodeShape ;
    sh:closed true ;
    sh:description "A management system certification or implementation with audit history and performance tracking." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class comp:AuditRecord ;
            sh:description "Audit history records" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path comp:audit_history ],
        [ sh:datatype xsd:string ;
            sh:description "Links to related commitments" ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path comp:commitment_alignment ],
        [ sh:datatype xsd:string ;
            sh:description "References to certificates and reports" ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path comp:evidence_documents ],
        [ sh:description "Type of management system" ;
            sh:in ( "QMS" "EMS" "OHS" "ISMS" "ENERGY" "CSR" "FSMS" "BCM" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path comp:system_type ],
        [ sh:class comp:StandardReference ;
            sh:description "Reference to the standard" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path comp:standard_reference ],
        [ sh:class comp:PerformanceMetrics ;
            sh:description "Performance metrics and targets" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path comp:performance_metrics ],
        [ sh:datatype xsd:string ;
            sh:description "Scope of the management system" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path comp:scope_description ],
        [ sh:class comp:CertificationDetails ;
            sh:description "Certification information" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path comp:certification_details ] ;
    sh:targetClass comp:ManagementSystem .

comp:PerformanceMetrics a sh:NodeShape ;
