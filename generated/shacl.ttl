comp:RequirementEntry a sh:NodeShape ;
    sh:closed true ;
    sh:description "A single regulation, standard, or specification requirement with multi-jurisdictional applicability and version tracking." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "References to supporting evidence" ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path comp:compliance_evidence_links ],
        [ sh:datatype xsd:string ;
            sh:description "Version or revision of the requirement" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path comp:version_value ],
        [ sh:datatype xsd:string ;
            sh:description "Unique identifier for this requirement" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path comp:requirement_identifier ],
        [ sh:class comp:Amendment ;
            sh:description "History of amendments to this requirement" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 12 ;
            sh:path comp:amendment_history ],
        [ sh:class comp:RequirementScope ;
            sh:description "Scope and applicability details" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 10 ;
            sh:path comp:requirement_scope ],
        [ sh:class comp:ApplicabilityStatus ;
            sh:description "Status of this requirement in different jurisdictions" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 6 ;
            sh:path comp:applicability_statuses ],
        [ sh:datatype xsd:string ;
            sh:description "References to related commitments" ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path comp:commitment_links ],
        [ sh:datatype xsd:string ;
            sh:description "Reference to newer requirement if this is superseded" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path comp:successor_reference ],
        [ sh:datatype xsd:string ;
            sh:description "List of harmonized or equivalent standards" ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path comp:harmonized_standards ],
        [ sh:datatype xsd:string ;
            sh:description "Authority that issued this requirement" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path comp:issuing_authority ],
        [ sh:datatype xsd:date ;
            sh:description "Date when this requirement was published" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path comp:publication_date ],
        [ sh:datatype xsd:string ;
            sh:description "Official title of the requirement" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path comp:title_value ],
        [ sh:description "Type of requirement (regulation, standard, specification)" ;
            sh:in ( "REGULATION" "DIRECTIVE" "STANDARD" "SPECIFICATION" "GUIDELINE" "TECHNICAL_REPORT" "CODE_OF_PRACTICE" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path comp:requirement_type ] ;
    sh:targetClass comp:RequirementEntry .

